.section_cart
	padding-top 40px
	padding-bottom 67px
	clearfix()

.cart-table
	margin-bottom 32px
	width 1150px
	background $white
	background rgba(255,255,255,.7)
	border-radius 17px
	box-shadow 0.15625rem 0.26875rem 6px rgba(0, 0, 0, 0.3)
	&__header
		padding 0 15px
		max-width 100%
		height 40px
		display table
		border-radius 17px 17px 0 0
		background-color #90aa3c
		font-iskrareg(20px,1)
		color $white
	&__cell
		display table-cell
		vertical-align middle
		&_title
			width 585px
			a
				color #3f4b1a
				text-decoration none
				&:hover
					text-decoration underline
		&_id
			padding-right 12px
			width 120px
			text-align center
		&_quant
			width 100px
			text-align center

		&_price
			padding-left 28px
			width 130px
			text-align center
		&_amount
			padding-right 8px
			width 130px
			text-align center
		&_delete
			padding-right 14px
			width 56px
			text-align center
	&__body
		padding-top 8px
		padding-left 16px
		padding-right 16px
	&__row
		padding-top 4px
		padding-bottom 5px
		border-bottom 1px solid #90aa3c
		font-iskrareg(20px,22px)
		color #3f4b1a

.summary
	margin-left 16px
	margin-right 16px
	border-top 1px solid #90aa3c
	clearfix()
	&__w
		float right
	&__row
		min-width 205px
		display table
		border-top 1px solid #90aa3c
		&:first-child
			border-top none
	&__item
		padding-left 10px
		display table-cell
		vertical-align middle
		font-iskrareg(20px,48px)
		&_title
			width 100px
			text-align right
		&_val
			width 105px
			text-align left

.cart-item
	margin-right 50px
	margin-bottom 27px
	background-color white
	background rgba(255,255,255,.7)
	border-radius 17px
	box-shadow 0.15625rem 0.26875rem 6px rgba(0, 0, 0, 0.3)
	&:nth-child(2n)
		margin-right 0
	&_half
		float left
		width 550px
	&__header
		padding 10px 16px 10px
		min-height 41px
		background-color #90aa3c
		border-radius 17px 17px 0 0
	&__title
		font-iskrareg(20px,1)
		color $white

	&__body
		padding 15px 16px 10px
	&__row
		margin-bottom 14px
		clearfix()
		&_style-2
			margin-bottom 22px
	&__i
		float left
		&_title
			width 149px
			text-align right
			font-iskrareg(20px,1)
			color #3f4b1a
			white-space nowrap
			sub
				color #c00000
				vertical-align top
		&_val
			margin-left 19px
			width 350px
	&__input
		input
			border none
			padding 8px 20px 4px
			width 100%
			height 34px
			background $white
			font-helvlght(17px,1)
			color #9b9b9b
			border-radius 13px
			box-shadow inset 1px 2px 6px rgba(0, 0, 0, 0.3)
	&__select
		position relative
		width 330px
		height 34px
		// background $white
		border-radius 13px
		box-shadow inset 1px 2px 6px rgba(0, 0, 0, 0.3)
		overflow hidden
		select
			position relative
			border none
			padding 0 20px
			width 100%
			height 34px
			background transparent
			font-helvlght(17px,34px)
			color #9b9b9b
			z-index 1
			cursor pointer
			border-radius 13px
	&__radio
		margin-top 9px
		margin-left 10px
		margin-right 18px
		float left
	.iradio
		position relative
		width 17px
		height 17px
		background transparent
		border 1px solid #3f4b1a
		border-radius 50%
		&.checked
			border 1px solid transparent
			background-color #90aa3c
			&:after
				position absolute
				top 50%
				left 50%
				content ""
				margin-top -0.15625rem
				margin-left -0.15625rem
				size 5px
				background-color $white
				box-shadow 0.03125rem 0.05625rem 2px rgba(18, 17, 12, 0.6)
				border-radius 50%
	&__label
		margin-top 6px
		float left
		display block
		vertical-align top
		font-iskrareg(20px,1)
		color #3f4b1a
		label
			cursor pointer
	&__textarea
		width 550px
		textarea
			padding 13px 14px
			border none
			width 100%
			min-height 150px
			font-helvlght(17px, 19px)
			border-radius 13px
			box-shadow inset 1px 2px 6px rgba(0, 0, 0, 0.3)

.section__row
	clearfix()

.cart-comment
	margin-right 0
	margin-bottom 30px
	float right
	box-shadow  0px 0px 0px rgba(0, 0, 0, 0.3)
.cart-item_client
.cart-address
	.cart-item__i_title
		text-align right
	.cart-item__label
		float none
.cart-address
	.cart-item__i_title
		float left
		width 170px
	.cart-item__i_val
		margin-left 10px
		float right
		width 330px
	// .cart-item__input



// Custom select
.ui-widget
	font-helvlght(17px,1)
	color #9b9b9b

.ui-selectmenu-button
	display block
	height 34px
.ui-state-default
	background $white
	border-radius 13px
	box-shadow inset 1px 2px 6px rgba(0, 0, 0, 0.3)

.ui-menu
	background $white
	border 1px solid #9b9b9b
	.ui-menu-item
		padding 7px 19px 5px 18px
		line-height 1.2
.cart-item__select
	.ui-selectmenu-button
		border none
		padding 9px 36px 5px 19px
		.ui-icon
			sprite($select_icon)
			+retina
				sprite2($select_icon_2x)
